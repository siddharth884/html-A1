    <h2>Some Mathematical Symbols Supported by HTML</h2>
    <table border="1" width="100%" cellspacing="0" cellpadding="5">
        <tr>
            <th>char</th>
            <th>Number</th>
            <th>Entity</th>
            <th>Description</th>
        </tr>
        <tr>
            <td>&forall;</td>
            <td>&#8704;</td>
            <td>&amp;forall;</td>
            <td>FOR ALL</td>
        </tr>
        <tr>
            <td>&part;</td>
            <td>&#8706;</td>
            <td>&amp;part;</td>
            <td>PARTIAL DIFFERENTIAL</td>
        </tr>
        <tr>
            <td>&exist;</td>
            <td>&#8707;</td>
            <td>&amp;exist;</td>
            <td>THERE EXISTS</td>
        </tr>
        <tr>
            <td>&empty;</td>
            <td>&#8709;</td>
            <td>&amp;empty;</td>
            <td>EMPTY SETS</td>
        </tr>
        <tr>
            <td>&nabla;</td>
            <td>&#8711;</td>
            <td>&amp;nabla;</td>
            <td>NABLA</td>
        </tr>
        <tr>
            <td>&isin;</td>
            <td>&#8712;</td>
            <td>&amp;isin;</td>
            <td>ELEMENT OF</td>
        </tr>
        <tr>
            <td>&notin;</td>
            <td>&#8713;</td>
            <td>&amp;notin;</td>
            <td>NOT AN ELEMENT OF</td>
        </tr>
        <tr>
            <td>&ni;</td>
            <td>&#8715;</td>
            <td>&amp;ni;</td>
            <td>CONTAINS AS MEMBER</td>
        </tr>
        <tr>
            <td>&prod;</td>
            <td>&#8719;</td>
            <td>&amp;prod;</td>
            <td>N-ARY PRODUCT</td>
        </tr>
        <tr>
            <td>&sum;</td>
            <td>&#8721;</td>
            <td>&amp;sum;</td>
            <td>N-ARY SUMMATION</td>
        </tr>
    </table>

